local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local validKey = "Premium_bdhuw3te36fyr36ret376"
local allowedUserId = 8238519054
local issuedDate = "2025-07-1 22"

local function parseDate(str)
	local y, m, d, h = str:match("(%d+)%-(%d+)%-(%d+) (%d+)")
	return os.time({year = tonumber(y), month = tonumber(m), day = tonumber(d), hour = tonumber(h)})
end

local issuedTimestamp = parseDate(issuedDate)
local now = os.time()
local expireAfter = 30 * 24 * 60 * 60
if now > (issuedTimestamp + expireAfter) then
	player:Kick("‚ùå Key expired. Please get a new key.")
	return
end

-- üî∑ Cleanup Function
local function cleanup()
	if playerGui:FindFirstChild("KeyGui") then
		playerGui.KeyGui:Destroy()
	end
	if playerGui:FindFirstChild("LauncherGui") then
		playerGui.LauncherGui:Destroy()
	end
end

-- üî∑ Launcher Icon
local launcherGui = Instance.new("ScreenGui", playerGui)
launcherGui.Name = "LauncherGui"

local iconButton = Instance.new("ImageButton", launcherGui)
iconButton.Size = UDim2.new(0, 50, 0, 50)
iconButton.Position = UDim2.new(0, 10, 1, -60)
iconButton.Image = "rbxassetid://133520378097790"
iconButton.BackgroundTransparency = 1

-- üî∑ Key GUI
local function showKeyGui()
	if playerGui:FindFirstChild("KeyGui") then return end

	local screenGui = Instance.new("ScreenGui", playerGui)
	screenGui.Name = "KeyGui"

	local frame = Instance.new("Frame", screenGui)
	frame.Name = "Frame"
	frame.Size = UDim2.new(0, 350, 0, 0)
	frame.Position = UDim2.new(0.5, 0, 1, 0)
	frame.AnchorPoint = Vector2.new(0.5, 0.5)
	frame.BackgroundTransparency = 0.2
	frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 20)

	TweenService:Create(frame, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
		Position = UDim2.new(0.5, 0, 0.5, 0),
		Size = UDim2.new(0, 350, 0, 240)
	}):Play()

	local icon = Instance.new("ImageLabel", frame)
	icon.Size = UDim2.new(0, 24, 0, 24)
	icon.Position = UDim2.new(0, 10, 0, 10)
	icon.BackgroundTransparency = 1
	icon.Image = "rbxassetid://133520378097790"

	local title = Instance.new("TextLabel", frame)
	title.Size = UDim2.new(1, -40, 0, 30)
	title.Text = "HLemonHub Key | Premium"
	title.TextColor3 = Color3.new(1,1,1)
	title.BackgroundTransparency = 1
	title.Font = Enum.Font.GothamBold
	title.TextSize = 22
	title.Position = UDim2.new(0, 40, 0, 8)
	title.TextXAlignment = Enum.TextXAlignment.Left

	local desc = Instance.new("TextLabel", frame)
	desc.Size = UDim2.new(1, -40, 0, 30)
	desc.Text = "Verify Your Key Premium Version and join discord to get premium key"
	desc.TextColor3 = Color3.new(1,1,1)
	desc.BackgroundTransparency = 1
	desc.Font = Enum.Font.Gotham
	desc.TextWrapped = true
	desc.TextSize = 14
	desc.Position = UDim2.new(0, 20, 0, 45)

	local close = Instance.new("TextButton", frame)
	close.Size = UDim2.new(0, 30, 0, 30)
	close.Position = UDim2.new(1, -35, 0, 5)
	close.Text = "‚úï"
	close.TextColor3 = Color3.new(1,0,0)
	close.Font = Enum.Font.Gotham
	close.TextSize = 20
	close.BackgroundTransparency = 1

	local textbox = Instance.new("TextBox", frame)
	textbox.Size = UDim2.new(0.8, 0, 0, 35)
	textbox.Position = UDim2.new(0.1, 0, 0.45, 0)
	textbox.PlaceholderText = "Enter your key..."
	textbox.Text = ""
	textbox.TextColor3 = Color3.new(0,0,0)
	textbox.Font = Enum.Font.Gotham
	textbox.TextSize = 18
	textbox.BackgroundColor3 = Color3.fromRGB(255,255,255)
	Instance.new("UICorner", textbox).CornerRadius = UDim.new(0, 8)

	local button = Instance.new("TextButton", frame)
	button.Size = UDim2.new(0.4, -5, 0, 35)
	button.Position = UDim2.new(0.1, 0, 0.7, 0)
	button.Text = "Check Key"
	button.TextColor3 = Color3.fromRGB(255,255,255)
	button.Font = Enum.Font.GothamBold
	button.TextSize = 18
	button.BackgroundColor3 = Color3.fromRGB(0,0,0)
	Instance.new("UICorner", button).CornerRadius = UDim.new(0, 8)

	local discordBtn = Instance.new("TextButton", frame)
	discordBtn.Size = UDim2.new(0.4, -5, 0, 35)
	discordBtn.Position = UDim2.new(0.5, 5, 0.7, 0)
	discordBtn.Text = "Join Discord"
	discordBtn.TextColor3 = Color3.fromRGB(255,255,255)
	discordBtn.Font = Enum.Font.GothamBold
	discordBtn.TextSize = 18
	discordBtn.BackgroundColor3 = Color3.fromRGB(88,101,242)
	Instance.new("UICorner", discordBtn).CornerRadius = UDim.new(0, 8)

	local result = Instance.new("TextLabel", frame)
	result.Size = UDim2.new(1, 0, 0, 20)
	result.Position = UDim2.new(0, 0, 1, -25)
	result.BackgroundTransparency = 1
	result.Text = ""
	result.TextColor3 = Color3.new(1,0,0)
	result.Font = Enum.Font.Gotham
	result.TextSize = 16

	close.MouseButton1Click:Connect(function()
		cleanup()
	end)

	button.MouseButton1Click:Connect(function()
		if textbox.Text == validKey then
			if player.UserId == allowedUserId then
				result.TextColor3 = Color3.new(0,1,0)
				result.Text = "‚úÖ Key Valid | Enjoy!"
				pcall(function()
					writefile("MyKey.txt", validKey)
				end)
				wait(1)
				cleanup()
				task.wait(0.5)
				loadstring(game:HttpGet('https://pastefy.app/pNYqnGYk/raw'))()
				loadstring(game:HttpGet('https://pastefy.app/fA2w9kZx/raw'))()
			else
				result.TextColor3 = Color3.new(1,0,0)
				result.Text = "‚ùå Key not registered for this account."
			end
		else
			result.TextColor3 = Color3.new(1,0,0)
			result.Text = "‚ùå Wrong key."
		end
	end)

	discordBtn.MouseButton1Click:Connect(function()
		setclipboard("https://discord.gg/ECxQFc97F5")
		result.TextColor3 = Color3.new(0,1,0)
		result.Text = "‚úÖ Discord link copied!"
	end)
end

-- üî∑ Tampilkan GUI Key langsung
showKeyGui()

-- üî∑ Kalau tekan icon ‚Üí tampil lagi
iconButton.MouseButton1Click:Connect(showKeyGui)
