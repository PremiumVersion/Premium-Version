result.Position = UDim2.new(0, 0, 1, -25)
result.BackgroundTransparency = 1
result.Text = ""
result.TextColor3 = Color3.new(1,0,0)
result.Font = Enum.Font.Gotham
result.TextSize = 16

local function animateClose()
	local tween = TweenService:Create(frame, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
		Size = UDim2.new(0, 0, 0, 0),
		BackgroundTransparency = 1
	})
	tween:Play()
	tween.Completed:Wait()
	screenGui:Destroy()
end

close.MouseButton1Click:Connect(function()
	animateClose()
end)

button.MouseButton1Click:Connect(function()
	if textbox.Text == validKey then
		if player.UserId == allowedUserId then
			result.TextColor3 = Color3.new(0,1,0)
			result.Text = "Key Valide | Enjoyy My Script"
			pcall(function()
				writefile("MyKey.txt", validKey)
			end)
			wait(1)
			animateClose()
			task.wait(0.5)
			loadstring(game:HttpGet('https://pastefy.app/pNYqnGYk/raw'))()
			loadstring(game:HttpGet('https://pastefy.app/fA2w9kZx/raw'))()
		else
			result.TextColor3 = Color3.new(1,0,0)
			result.Text = "key not valid | This key is not registered for your account."
		end
	else
		result.TextColor3 = Color3.new(1,0,0)
		result.Text = "Wrong key."
	end
end)

discordBtn.MouseButton1Click:Connect(function()
	setclipboard("https://discord.gg/ECxQFc97F5")
	result.TextColor3 = Color3.new(0,1,0)
	result.Text = "Discord link copied!"
end)
