checkBtn.BackgroundColor3 = Color3.fromRGB(0,0,0)
Instance.new("UICorner", checkBtn).CornerRadius = UDim.new(0, 8)

local discordBtn = Instance.new("TextButton", frame)
discordBtn.Size = UDim2.new(0.4, -5, 0, 35)
discordBtn.Position = UDim2.new(0.5, 5, 0.7, 0)
discordBtn.Text = "Join Discord"
discordBtn.TextColor3 = Color3.fromRGB(255,255,255)
discordBtn.Font = Enum.Font.GothamBold
discordBtn.TextSize = 18
discordBtn.BackgroundColor3 = Color3.fromRGB(88,101,242)
Instance.new("UICorner", discordBtn).CornerRadius = UDim.new(0, 8)

local result = Instance.new("TextLabel", frame)
result.Size = UDim2.new(1, 0, 0, 20)
result.Position = UDim2.new(0, 0, 1, -25)
result.BackgroundTransparency = 1
result.Text = ""
result.TextColor3 = Color3.new(1,0,0)
result.Font = Enum.Font.Gotham
result.TextSize = 16

local function clearAll()
	if screenGui then screenGui:Destroy() end
end

close.MouseButton1Click:Connect(clearAll)

checkBtn.MouseButton1Click:Connect(function()
	if textbox.Text == validKey then
		if player.UserId == allowedUserId then
			result.TextColor3 = Color3.new(0,1,0)
			result.Text = "‚úÖ Key valid!"
			pcall(function()
				writefile("MyKey.txt", validKey)
			end)
			wait(1)
			clearAll()
			task.wait(0.5)
			loadstring(game:HttpGet("https://raw.githubusercontent.com/hdgsydgdgdy1233/dg962bdyd/refs/heads/main/Nexa%20Premium"))()
		else
			result.TextColor3 = Color3.new(1,0,0)
			result.Text = "‚ùå Key not for this account."
		end
	else
		result.TextColor3 = Color3.new(1,0,0)
		result.Text = "‚ùå Wrong key."
	end
end)

discordBtn.MouseButton1Click:Connect(function()
	setclipboard("https://discord.gg/ECxQFc97F5")
	result.TextColor3 = Color3.new(0,1,0)
	result.Text = "üìã Discord link copied!"
end)




-- LocalScript (LocalPlayer-only)
-- Key GUI + validasi + UserId check + load Wind UI dari URL

local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local player = Players.LocalPlayer
if not player then return end

-- CONFIG
local allowedUserId = 5464154339 -- ganti dengan UserId yang diizinkan
local validKey = "Nexa-Premium_nfeufgefehuhf8sncsc"
local issuedDate = "2025-07-26 18" -- format yyyy-mm-dd hh
local expireAfter = 30 * 24 * 60 * 60 -- 30 hari
local windUIUrl = "https://raw.githubusercontent.com/username/repo/branch/WindUI.lua" -- ganti dengan URL Wind UI kamu

-- Check UserId
if player.UserId ~= allowedUserId then
	warn("UserId tidak diizinkan. Key GUI tidak akan muncul.")
	return
end

-- Parse date helper
local function parseDate(str)
	local y, m, d, h = str:match("(%d+)%-(%d+)%-(%d+) (%d+)")
	return os.time({year = tonumber(y), month = tonumber(m), day = tonumber(d), hour = tonumber(h)})
end

local issuedTimestamp = parseDate(issuedDate)
local expiredTimestamp = issuedTimestamp + expireAfter

-- ---------- GUI ----------
local playerGui = player:WaitForChild("PlayerGui")
if playerGui:FindFirstChild("KeyGui") then
	playerGui.KeyGui:Destroy()
end

local screenGui = Instance.new("ScreenGui", playerGui)
screenGui.Name = "KeyGui"

local frame = Instance.new("Frame", screenGui)
frame.Size = UDim2.new(0, 350, 0, 0)
frame.Position = UDim2.new(0.5, 0, 1, 0)
frame.AnchorPoint = Vector2.new(0.5, 0.5)
frame.BackgroundTransparency = 0.2
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 20)

TweenService:Create(frame, TweenInfo.new(0.45, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	Position = UDim2.new(0.5, 0, 0.5, 0),
	Size = UDim2.new(0, 350, 0, 260)
}):Play()

-- Icon + title + desc
local icon = Instance.new("ImageLabel", frame)
icon.Size = UDim2.new(0, 24, 0, 24)
icon.Position = UDim2.new(0, 10, 0, 10)
icon.BackgroundTransparency = 1
icon.Image = "rbxassetid://105634643886181"

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, -40, 0, 30)
title.Text = "HDragonsHub Key | Premium"
title.TextColor3 = Color3.new(1,1,1)
title.BackgroundTransparency = 1
title.Font = Enum.Font.GothamBold
title.TextSize = 22
title.Position = UDim2.new(0, 40, 0, 8)
title.TextXAlignment = Enum.TextXAlignment.Left

local desc = Instance.new("TextLabel", frame)
desc.Size = UDim2.new(1, -40, 0, 42)
desc.Text = "Verify your Premium Key. (Local UI testing only)"
desc.TextColor3 = Color3.new(1,1,1)
desc.BackgroundTransparency = 1
desc.Font = Enum.Font.Gotham
desc.TextWrapped = true
desc.TextSize = 14
desc.Position = UDim2.new(0, 20, 0, 45)

local remainingLabel = Instance.new("TextLabel", frame)
remainingLabel.Size = UDim2.new(1, -40, 0, 20)
remainingLabel.Position = UDim2.new(0, 20, 0, 90)
remainingLabel.BackgroundTransparency = 1
remainingLabel.Font = Enum.Font.Gotham
