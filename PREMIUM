local result = Instance.new("TextLabel", frame)
result.Size = UDim2.new(1, 0, 0, 20)
result.Position = UDim2.new(0, 0, 1, -25)
result.BackgroundTransparency = 1
result.Text = ""
result.TextColor3 = Color3.new(1,0,0)
result.Font = Enum.Font.Gotham
result.TextSize = 16

frame.Active = true
frame.Draggable = true

-- ---------- Functions ----------
local function flashResult(color,text)
	result.TextColor3 = color
	result.Text = text
end

local function formatRemaining(secs)
	if secs <= 0 then
		return "Expired"
	end
	local days = math.floor(secs/86400)
	local hours = math.floor((secs%86400)/3600)
	local mins = math.floor((secs%3600)/60)
	return string.format("%d hari %02d jam %02d menit", days,hours,mins)
end

local function updateRemainingLabel()
	local now = os.time()
	local rem = math.max(0, expiredTimestamp - now)
	remainingLabel.Text = "Sisa waktu key: "..formatRemaining(rem)
	if rem <= 0 then
		remainingLabel.TextColor3 = Color3.fromRGB(255,100,100)
	else
		remainingLabel.TextColor3 = Color3.fromRGB(200,200,200)
	end
end

task.spawn(function()
	while frame and frame.Parent do
		updateRemainingLabel()
		task.wait(10)
	end
end)

-- ---------- Events ----------
close.MouseButton1Click:Connect(function()
	if screenGui and screenGui.Parent then screenGui:Destroy() end
end)

checkBtn.MouseButton1Click:Connect(function()
	local input = tostring(textbox.Text):match("^%s*(.-)%s*$") or ""
	if input == "" then
		flashResult(Color3.fromRGB(255,100,100), "‚ùå Masukkan key terlebih dahulu.")
		return
	end
	if input == validKey then
		flashResult(Color3.fromRGB(0,1,0), "‚úÖ Key valid! Loading WindUI...")
		task.delay(0.1,function()
			pcall(function()
				loadstring(game:HttpGet(windUIUrl))()
			end)
		end)
		TweenService:Create(frame, TweenInfo.new(0.35, Enum.EasingStyle.Quad, Enum.EasingDirection.In),{
			Size=UDim2.new(0,350,0,0),
			Position=UDim2.new(0.5,0,1,0)
		}):Play()
		task.delay(0.4,function()
			if screenGui and screenGui.Parent then screenGui:Destroy() end
		end)
	else
		flashResult(Color3.fromRGB(1,0,0), "‚ùå Wrong key.")
	end
end)

discordBtn.MouseButton1Click:Connect(function()
	result.TextColor3 = Color3.fromRGB(1,1,1)
	result.Text = "üîó Discord: https://discord.gg/ECxQFc97F5"
end)
