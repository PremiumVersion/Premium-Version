local TweenService = game:GetService("TweenService")
local StarterGui = game:GetService("StarterGui")

-- üî∑ Key & UserId
local validKey = "Premium_VsnGGSECRETKEYhfggr3tfr"
local allowedUserId = 8309815866
local issuedDate = "2025-07-10 22"

local function parseDate(str)
	local y, m, d, h = str:match("(%d+)%-(%d+)%-(%d+) (%d+)")
	return os.time({year = tonumber(y), month = tonumber(m), day = tonumber(d), hour = tonumber(h)})
end

local issuedTimestamp = parseDate(issuedDate)
local now = os.time()
local expireAfter = 30 * 24 * 60 * 60
if now > (issuedTimestamp + expireAfter) then
	game.Players.LocalPlayer:Kick("‚ùå Key expired. Please get a new key.")
	return
end

local hasKey
pcall(function()
	if isfile("MyKey.txt") then
		local savedKey = readfile("MyKey.txt")
		if savedKey == validKey and game.Players.LocalPlayer.UserId == allowedUserId then
			hasKey = true
		end
	end
end)

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

if playerGui:FindFirstChild("KeyGui") then
	playerGui.KeyGui:Destroy()
end

local screenGui = Instance.new("ScreenGui", playerGui)
screenGui.Name = "KeyGui"

local frame = Instance.new("Frame", screenGui)
frame.Size = UDim2.new(0, 0, 0, 0)
frame.Position = UDim2.new(0.5, 0, 0.5, 0)
frame.AnchorPoint = Vector2.new(0.5, 0.5)
frame.BackgroundTransparency = 0
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)

local corner = Instance.new("UICorner", frame)
corner.CornerRadius = UDim.new(0, 20)

TweenService:Create(frame, TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
	Size = UDim2.new(0, 300, 0, 150)
}):Play()

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, -30, 0, 30)
title.Text = "HLemonHub Key | Premium Version"
title.TextColor3 = Color3.new(1,1,1)
title.BackgroundTransparency = 1
title.Font = Enum.Font.SourceSansBold
title.TextSize = 20
title.Position = UDim2.new(0, 0, 0, 0)

local close = Instance.new("TextButton", frame)
close.Size = UDim2.new(0, 30, 0, 30)
close.Position = UDim2.new(1, -30, 0, 0)
close.Text = "‚ùå"
close.TextColor3 = Color3.new(1,0,0)
close.Font = Enum.Font.SourceSansBold
close.TextSize = 20
close.BackgroundTransparency = 1

local textbox = Instance.new("TextBox", frame)
textbox.Size = UDim2.new(0.8, 0, 0, 30)
textbox.Position = UDim2.new(0.1, 0, 0.4, 0)
textbox.PlaceholderText = "Type your key..."
textbox.Text = validKey
textbox.TextColor3 = Color3.new(0,0,0)
textbox.Font = Enum.Font.SourceSans
textbox.TextSize = 18
textbox.BackgroundColor3 = Color3.fromRGB(255,255,255)

local tbCorner = Instance.new("UICorner", textbox)
tbCorner.CornerRadius = UDim.new(0, 8)

local button = Instance.new("TextButton", frame)
button.Size = UDim2.new(0.5, 0, 0, 30)
button.Position = UDim2.new(0.25, 0, 0.75, 0)
button.Text = "Submit"
button.TextColor3 = Color3.fromRGB(255,255,255)
button.Font = Enum.Font.SourceSansBold
button.TextSize = 18
button.BackgroundColor3 = Color3.fromRGB(0,170,0)

local btnCorner = Instance.new("UICorner", button)
btnCorner.CornerRadius = UDim.new(0, 8)

local result = Instance.new("TextLabel", frame)
result.Size = UDim2.new(1, 0, 0, 20)
result.Position = UDim2.new(0, 0, 1, 5)
result.BackgroundTransparency = 1
result.Text = ""
result.TextColor3 = Color3.new(1,0,0)
result.Font = Enum.Font.SourceSans
result.TextSize = 16

local function animateClose()
	local tween = TweenService:Create(frame, TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
		Size = UDim2.new(0, 0, 0, 0),
		BackgroundTransparency = 1
	})
	tween:Play()
	tween.Completed:Wait()
	screenGui:Destroy()
end

close.MouseButton1Click:Connect(function()
	animateClose()
end)

button.MouseButton1Click:Connect(function()
	if textbox.Text == validKey then
		if player.UserId == allowedUserId then
			result.TextColor3 = Color3.new(0,1,0)
			result.Text = "‚úÖ Key accepted! Saving..."
			pcall(function()
				writefile("MyKey.txt", validKey)
			end)

			StarterGui:SetCore("SendNotification", {
				Title = "HLemonHub",
				Text = "‚úÖ Key valid! Loading script...",
				Duration = 3
			})

			wait(1)
			animateClose()
			task.wait(0.5)
			loadstring(game:HttpGet('https://pastefy.app/pNYqnGYk/raw'))()
			loadstring(game:HttpGet('https://pastefy.app/fA2w9kZx/raw'))()
		else
			result.TextColor3 = Color3.new(1,0,0)
			result.Text = "‚ùå This key is not registered for your account."
		end
	else
		result.TextColor3 = Color3.new(1,0,0)
		result.Text = "‚ùå Wrong key."
	end
end)
